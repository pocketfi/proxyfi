{
  "compilerOptions": {
	"target": "es5",
	"module": "commonjs",
	"moduleResolution": "node",
	"esModuleInterop": true,
	"strict": true,
	"outDir": "dist",
	"sourceMap": true,
	"lib": [
	  "dom",
	  "es5",
	  "scripthost",
	  "es2015.symbol.wellknown",
	  "es2015.collection"
	]
  },
  "include": [
	"./**/*"
  ],
  "exclude": [
	"../node_modules"
  ]
}
